{
  "name": "max-ui",
  "version": "0.2.1",
  "description": "Max's reusable React component library.",
  "author": "",
  "license": "ISC",
  "main": "dist/max-ui.cjs",
  "module": "dist/max-ui.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/max-ui.mjs",
      "require": "./dist/max-ui.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build:types": "tsc --project tsconfig.build.json",
    "build": "npm run clean && npm run build:types && vite build",
    "preview": "vite preview",
    "test": "echo \"No tests yet\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "design-system",
    "components"
  ],
  "dependencies": {
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.1.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.1.0",
    "sass": "^1.94.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
}
